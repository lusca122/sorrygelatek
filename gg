                eul1 = math.random(-50, 50)
                eul2 = math.random(-50, 50)
                eul3 = math.random(-50, 50)
                euld = CFrame.fromEulerAnglesXYZ(eul1, eul2, eul3)
                euld2 = CFrame.fromEulerAnglesXYZ(-eul1, -eul2, -eul3)
