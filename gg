                      bv.velocity = Head.CFrame.lookVector * 100 + Vector3.new(0, -2, 0)
                      spawn(function()
        wait()
        bv:Destroy()
      end
  )
                      mag = (MainPos - pos).magnitude
                      mag = (MainPos - pos).magnitude
