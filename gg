              v.Transparency = i
            end
          end
        end
      end
  ))
          if Beam.Transparency <= i and lapse > 1 then
            Beam.Transparency = i
            hitRef.Transparency = i
          end
          if lapse > 1 then
            Beam.Transparency = 0.4
            hitRef.Transparency = 0.4
          end
