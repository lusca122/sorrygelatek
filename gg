                if h and h.Health > 0 then
                  Damagefunc(hitRef, h, 2, 4, 0.7, "Normal", RootPart, 0.2, 1, nil, 2)
                end
                if hit.Anchored == false and h and h.Health <= 0 then
                  local bv = Instance.new("BodyVelocity", hit)
