                      local gunpos = Vector3.new(mouse.Hit.p.x, Head.Position.Y, mouse.Hit.p.z)
                      offset = (Torso.Position.y - mouse.Hit.p.y) / 40
                      local mag2 = (Torso.Position - mouse.Hit.p).magnitude / 80
                      offset = offset / mag2
                      gyro.cframe = cn(Head.Position, gunpos)
