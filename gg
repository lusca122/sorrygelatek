      label.FontSize = Enum.FontSize.Size32
      label.Font = Enum.Font.SourceSansBold
      label.BorderSizePixel = 0
      label.TextScaled = true
      label.Text = text
    end
  
    local colorscheme2 = Torso.BrickColor.Color
